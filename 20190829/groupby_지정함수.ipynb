   "source": [
    "import pandas as pd\n",
    "\n",
    "df=pd.read_excel('groupby_예제.xlsx')\n",
    "\n",
    "price_group=df['price'].groupby(df.index)\n",
    "\n",
    "def calc(group):\n",
    "    return max(group)*min(group)\n",
    "\n",
    "price_group.agg(calc)"
   ]
